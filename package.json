{
  "name": "code-samples",
  "version": "0.0.1",
  "description": "code-samples",
  "main": "src/main.jsx",
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "sideEffects": [
    "**/*.scss",
    "./vendor/benchmark.js"
  ],
  "scripts": {
    "start": "node ./scripts/server.js",
    "serve": "npm run serve:dev",
    "serve:dev": "node_modules/.bin/cross-env NODE_ENV=development webpack serve --config=build-scripts/webpack.config.js",
    "serve:prod": "node_modules/.bin/cross-env NODE_ENV=production webpack serve --config=build-scripts/webpack.config.js",
    "build": "npm run build:prod",
    "build:dev": "node_modules/.bin/cross-env NODE_ENV=development webpack --config=build-scripts/webpack.config.js",
    "build:prod": "node_modules/.bin/cross-env NODE_ENV=production webpack --config=build-scripts/webpack.config.js",
    "build:bundle-stats": "node_modules/.bin/cross-env NODE_ENV=production webpack --profile --json=stats/bundle.json --config=build-scripts/webpack.config.js",
    "analyze:bundle": "npm run build:prod && npm run build:bundle-stats && webpack-bundle-analyzer stats/bundle.json build/",
    "lint": "eslint --ext .js,.jsx src/ build-scripts/ babel.config.js",
    "lint:fix": "eslint --ext .js,.jsx src/ build-scripts/ babel.config.js --fix",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "stylelint src/**/*.{css,scss} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant478/code-samples.git"
  },
  "author": "ant478",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ant478/code-samples/issues"
  },
  "homepage": "https://github.com/ant478/code-samples#readme",
  "browserslist": [
    "last 10 chrome version"
  ],
  "dependencies": {
    "benchmark": "2.1.4",
    "classnames": "2.3.1",
    "core-js": "3.17.2",
    "cross-env": "7.0.3",
    "express": "4.18.0",
    "heroku-ssl-redirect": "0.1.1",
    "highlight.js": "11.2.0",
    "lodash": "4.17.21",
    "process": "0.11.10",
    "react": "17.0.2",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "17.0.2",
    "react-number-format": "4.9.2",
    "react-router-dom": "5.3.0",
    "react-router-hash-link": "2.4.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "1.7.0",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.2.2",
    "node-sass-glob-importer": "5.3.2",
    "postcss-loader": "6.1.1",
    "postcss-preset-env": "6.7.0",
    "reset-css": "5.0.1",
    "sass": "1.39.0",
    "sass-loader": "12.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "terser-webpack-plugin": "5.2.5",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.1.1",
    "webpack-merge": "5.8.0"
  }
}
